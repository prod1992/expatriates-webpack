    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "bootstrap-sass": "^3.4.1",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "select2": "^4.0.13"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "ajv": "^6.10.2",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
